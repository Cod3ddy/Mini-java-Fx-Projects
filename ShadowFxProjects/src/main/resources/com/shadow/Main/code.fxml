<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="460.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.shadow.controllers.CodeController">
   <children>
      <TextArea fx:id="textArea" layoutX="50.0" layoutY="86.0" prefHeight="283.0" prefWidth="493.0" wrapText="true">
         <font>
            <Font name="Arial" size="26.0" />
         </font>
      </TextArea>
      <Button fx:id="startButton" layoutX="251.0" layoutY="396.0" mnemonicParsing="false" onAction="#startWriting" prefHeight="49.0" prefWidth="90.0" text="Start">
         <font>
            <Font name="Arial" size="19.0" />
         </font>
      </Button>
      <Label fx:id="text" layoutX="282.0" layoutY="42.0" text="Label" />
   </children>
</AnchorPane>
